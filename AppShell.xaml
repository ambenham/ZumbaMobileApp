<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ZumbaMobileApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ZumbaMobileApp"
    Shell.FlyoutBehavior="Disabled">

    <ShellContent
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />

    <Shell.ToolbarItems>
        <ToolbarItem IconImageSource="settings_icon.svg"/>
        <ToolbarItem Order="Secondary" Priority="0" Text="My Classes"/>
        <ToolbarItem Order="Secondary" Priority="3" Text="Log Out"/>
    </Shell.ToolbarItems>

</Shell>
